{
	"name": "aesop",
	"displayName": "Aesop",
	"description": "An IDE-native suite for Storybook.",
	"version": "0.0.1",
	"publisher": "aesop",
	"repository": "https://github.com/Async-Aesop/aesop",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.aesopAwaken",
		"onCommand:extension.tollTheHour",
		"onCommand:extension.getStories"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.aesopAwaken",
				"title": "Aesop Awaken"
			},
			{
				"command": "extension.tollTheHour",
				"title": "Toll the Hour"
			},
			{
				"command": "extension.getStories",
				"title": "Get Storybook Stories"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"npm": "^6.13.4",
		"vsce": "^1.71.0",
		"vscode": "^1.1.36"
	}
}
